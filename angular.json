{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": true
    }
  },
  "projects": {
    "loanpost": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "css",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/loanpost",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.htaccess",
              "src/sitemap.xml",
              "src/robots.txt",
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "firebase",
              "firebase/app",
              "firebase/auth",
              "firebase/firestore",
              "firebase/functions",
              "firebase/app-check",
              "firebase/analytics",
              "firebase/messaging",
              "@firebase/app",
              "@firebase/auth",
              "@firebase/firestore",
              "@firebase/functions",
              "@firebase/app-check",
              "@firebase/analytics",
              "@firebase/messaging",
              "stripe",
              "@stripe/stripe-js",
               "typed-usa-states/dist/states-with-counties",
              "lodash",
              "rxjs",
              "rxjs/operators",
              "date-fns",
              "moment"
            ],
            "preserveSymlinks": true,
            "extractLicenses": false,
            "sourceMap": false,
            "optimization": {
              "scripts": true,
              "styles": {
                "minify": true,
                "inlineCritical": false
              },
              "fonts": {
                "inline": false
              }
            },
            "namedChunks": true,
            "vendorChunk": true,
            "buildOptimizer": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb",
                  "maximumError": "200kb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "vendorChunk": true,
              "buildOptimizer": true,
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": {
                  "inline": false
                }
              },
              "extractLicenses": true,
              "preserveSymlinks": false
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": {
                  "inline": false
                }
              },
              "extractLicenses": true,
              "sourceMap": false,
              "vendorChunk": true,
              "namedChunks": true,
              "buildOptimizer": true
            },
            "staging": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": {
                  "inline": false
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "loanpost:build:development",
            "port": 4200,
            "host": "localhost",
            "open": true,
            "liveReload": true,
            "hmr": false
          },
          "configurations": {
            "production": {
              "buildTarget": "loanpost:build:production",
              "port": 4200
            },
            "development": {
              "buildTarget": "loanpost:build:development",
              "port": 4200
            },
            "staging": {
              "buildTarget": "loanpost:build:staging",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "loanpost:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.htaccess",
              "src/sitemap.xml",
              "src/robots.txt",
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "codeCoverage": false,
            "watch": false,
            "browsers": "ChromeHeadless"
          },
          "configurations": {
            "watch": {
              "watch": true,
              "browsers": "Chrome"
            },
            "coverage": {
              "codeCoverage": true,
              "browsers": "ChromeHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/loanpost/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "optimization": true,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "loanpost:build:development",
              "serverTarget": "loanpost:server:development"
            },
            "production": {
              "browserTarget": "loanpost:build:production",
              "serverTarget": "loanpost:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "routes": [
              "/",
              "/login",
              "/pricing",
              "/terms",
              "/privacy"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "loanpost:build:production",
              "serverTarget": "loanpost:server:production"
            },
            "development": {
              "browserTarget": "loanpost:build:development",
              "serverTarget": "loanpost:server:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "analyze": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/loanpost",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.htaccess",
              "src/sitemap.xml",
              "src/robots.txt"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "statsJson": true,
            "namedChunks": true
          }
        }
      }
    }
  }
}